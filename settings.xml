<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <profiles>
        <profile>
            <repositories>
                <repository>
                    <id>actual-public-mvn</id>
                    <url>https://repo1.maven.org/maven2</url>
                </repository>
                <repository>
                    <id>gelreportmodel-gitlab</id>
                    <url>https://gitlab.com/api/v4/projects/11692019/packages/maven</url>
                </repository>
            </repositories>
            <id>gitlab</id>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>gitlab</activeProfile>
    </activeProfiles>
    <servers>
        <server>
            <id>gelreportmodel-gitlab</id>
            <configuration>
                <httpHeaders>
                    <property>
                        <name>Private-Token</name>
                        <value>ihK1bb-_fasieLsfmLwn</value>
                    </property>
                </httpHeaders>
            </configuration>
        </server>
    </servers>

    <pluginGroups>
        <!-- pluginGroup
         | Specifies a further group identifier to use for plugin lookup.
        <pluginGroup>com.your.plugins</pluginGroup>
        -->
    </pluginGroups>

    <!-- proxies
     | This is a list of proxies which can be used on this machine to connect to
     | the network. Unless otherwise specified (by system property or command-
     | line switch), the first proxy specification in this list marked as active
     | will be used.
     |-->
    <proxies>
        <proxy>
            <id>optional</id>
            <active>true</active>
            <protocol>http</protocol>
            <username>proxyuser</username>
            <password>proxypass</password>
            <host>proxy-dmz.gel.zone</host>
            <port>80</port>
            <nonProxyHosts>localhost|127.0.0.1|localaddress|.localdomain.com|.gel.zone|.genomicsengland.nhs.uk</nonProxyHosts>
        </proxy>
    </proxies>

</settings>